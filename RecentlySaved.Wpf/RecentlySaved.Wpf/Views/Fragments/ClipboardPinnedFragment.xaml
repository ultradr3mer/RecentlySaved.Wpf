<UserControl x:Class="RecentlySaved.Wpf.Views.Fragments.ClipboardPinnedFragment"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:RecentlySaved.Wpf.Views.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             xmlns:designTime="clr-namespace:RecentlySaved.Wpf.ViewModels.Fragments.DesignTime"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             d:DataContext ="{d:DesignInstance {x:Type designTime:ClipHistFragmentViewModelDesignTime}, IsDesignTimeCreatable=True}" >
  <controls:ClipCardList ItemsSource="{Binding Items}" SelectedItem="{Binding SelectedItem}"/>
</UserControl>
